{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=127.0.0.1;Initial Catalog='mySimpleDB';Integrated Security=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  // требуется ли загрузка демо данных при первом создании базы данных
  "HasDemoData": true,

  // разрешение на регистрацию через Web интерфейс
  "AllowedWebRegistration": false,
  // разрешение на авторизацию через Web интерфейс
  "AllowedWebLogin": true,

  "reCaptchaV2PublicKey": "xxxyyyxxxyyyxxxyyyxxxyyyxxxyyy",
  "reCaptchaV2PrivatKey": "xxxyyyxxxyyyxxxyyyxxxyyyxxxyyy",

  //"reCaptchaV2InvisiblePublicKey": "xxxyyyxxxyyyxxxyyyxxxyyyxxxyyy",
  //"reCaptchaV2InvisiblePrivatKey": "xxxyyyxxxyyyxxxyyyxxxyyyxxxyyy",

  // Id пользователя, которому следует назначит права ROOT. При запуске приложения будет произведена попытка найти такого пользователя и назначить ему ROOT права
  "SetUserRootById": 1
}
