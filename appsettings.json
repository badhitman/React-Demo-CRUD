{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=127.0.0.1;Initial Catalog='mySimpleDB';Integrated Security=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  // требуется ли загрузка демо данных в базу данных
  "HasDemoData": true,

  // разрешение на регистрацию через Web интерфейс
  "AllowedWebRegistration": false,
  // разрешение на авторизацию через Web интерфейс
  "AllowedWebLogin": true,

  "reCaptchaV2PublicKey": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
  "reCaptchaV2PrivatKey": "yyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyy",

  //"reCaptchaV2InvisiblePublicKey": "zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz",
  //"reCaptchaV2InvisiblePrivatKey": "wwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwww",

  // Id пользователя, которому следует назначит права ROOT.
  // Если значение больше нуля, то при запуске приложения будет произведена попытка найти такого пользователя и назначить ему роль: root.
  "SetUserRootById": 0
}
