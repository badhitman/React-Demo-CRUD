{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=127.0.0.1;Initial Catalog='debugDataBaseMyReactApp';Integrated Security=True; MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  //"reCaptchaV2PublicKey": "6LdIhEcUAAAAAMO-wXB9GLQCK1UJyj6OV47yjRXo",
  //"reCaptchaV2PrivatKey": "6LdIhEcUAAAAAJWS2SfCIrmYPDkeP-u7rvONbC-9",

  //"reCaptchaV2InvisiblePublicKey": "6Ld5eLgUAAAAAImQ1H3X1GkBgfZI-g_ojri1Gk3z",
  //"reCaptchaV2InvisiblePrivatKey": "6Ld5eLgUAAAAAGIqamBF-6Otg2dlU6Fc4L--OW6U",

  // разрешение на регистрацию через Web интерфейс
  "AllowedWebRegistration": true,
  // разрешение на авторизацию через Web интерфейс
  "AllowedWebLogin": true,

  // срок жизни сессии и связаных с ней кукисов
  "SessionCookieExpiresSeconds": 3600,

  // Передавать cookie через Secure Sockets Layer (SSL) - то есть только по протоколу HTTPS
  "SessionCookieSslSecureOnly": false,

  // требуется ли загрузка демо данных в базу данных
  "HasDemoData": true,

  // Id пользователя, которому следует назначит права ROOT.
  // Если значение больше нуля, то при запуске приложения будет произведена попытка найти такого пользователя и назначить ему роль: root.
  "SetUserRootById": 0,

  // Папка загружаемых файлов  (new DirectoryInfo(AppOptions.UploadsPatch))
  "UploadsPath": "uploads",

  // Высота thumb картинки
  "ThumbMaxHeight": 150,
  // Ширина thumb картинки
  "ThumbMinHeight": 150,

  // Системная учётная запись. От её имени совершаются системные события.
  "SystemUser": 1,

  // Максимальное количество строк в документе (табличная часть)
  "MaxNumRowsDocument": 128
}
